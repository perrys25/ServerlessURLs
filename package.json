{
  "name": "serverlessurls",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:auth": "concurrently \"npm run keycloak\" \"npm run dev\" -n \"keycloak,next\" --hide \"keycloak\" --kill-others-on-fail true",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev --port 3000 --kv=URLS_KV --d1 AUTH_D1",
    "auth": "concurrently \"npm run keycloak\" \"npm run preview\" -n \"keycloak,next\" --hide \"keycloak\" --kill-others-on-fail true",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "keycloak": "sh ./keycloak/bin/kc.sh start-dev --hostname http://localhost:8080"
  },
  "dependencies": {
    "@auth/d1-adapter": "^1.4.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "boring-avatars": "^1.10.2",
    "is-url-http": "^2.3.8",
    "next": "^14.2",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.11.3",
    "@cloudflare/workers-types": "^4.20240614.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "^14.2",
    "eslint-plugin-next-on-pages": "^1.11.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vercel": "^34.2.7",
    "wrangler": "^3.61.0"
  }
}
